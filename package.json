  "scripts": {
    "build:client": "webpack -c ./config/webpack/client.js",
    "build:server": "webpack -c ./config/webpack/server.js",
    "start:client": "yarn build:client --watch",
    "start:server": "yarn build:client && yarn build:server --watch",
    "store-api": "yarn remove store-api && yarn add https://github.com/EvgenyiFedotov/store-api.git",
    "store-api-react": "yarn remove store-api-react && yarn add https://github.com/EvgenyiFedotov/store-api-react.git"
  },
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/express": "^4.17.11",
