    "@types/react-dom": "^17.0.0",
    "css-loader": "^5.0.1",
    "mini-css-extract-plugin": "^1.3.4",
    "nodemon-webpack-plugin": "^4.4.4",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "antd": "^4.10.2",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
